<!DOCTYPE html>
<html lang="en">
  <%- include('./particals/head.ejs') %>
  <body class="reg">
    <%- include('./particals/loginNav.ejs') %>
    <div class="login">
      <div class="login-container">
        <div class="header-login">
          <h1>Login</h1>
        </div>
        <div class="form-section">
          <form action="#">
            <div class="email">
              <h3>Email</h3>
              <input
                type="email"
                id="loginEmail"
                placeholder="abc@xyz.com"
                required
                autocomplete="off"
              />
            </div>
            <div class="password">
              <h3>Password</h3>
              <input
                type="password"
                name="password"
                id="pass"
                placeholder="Your password"
                autocomplete="off"
                required
              />
            </div>
            <div class="submit">
              <input type="submit" id="sub" value="Login" />
            </div>
          </form>
        </div>
      </div>
    </div>
    <script src="./app.js"></script>
  </body>
</html>
